{
    "name": "formandsystem/api",
    "description": "API Client package for Form&System API.",
    "keywords": [
        "league",
        ":package_name"
    ],
    "homepage": "https://github.com/formandsystem/api",
    "license": "MIT",
    "authors": [
        {
            "name": "Lukas Oppermann",
            "email": "lukas@formandsystem.com",
            "homepage": "http://formandsystem.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Formandsystem\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Formandsystem\\Api\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
